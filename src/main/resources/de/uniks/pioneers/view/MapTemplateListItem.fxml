<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>


<HBox prefHeight="20.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.uniks.pioneers.controller.MapTemplateSubcontroller">
    <Label fx:id="nameLabel" text="NAME"/>
    <Label fx:id="createdByLabel" text="CREATED_BY">
        <HBox.margin>
          <Insets left="140.0"/>
        </HBox.margin>
    </Label>
    <Label fx:id="votesLabel" text="VOTES">
        <HBox.margin>
          <Insets left="140.0"/>
        </HBox.margin>
    </Label>
    <ImageView fx:id="selectedImageView" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
        <HBox.margin>
          <Insets left="24.0"/>
        </HBox.margin>
    </ImageView>
    <Button fx:id="showButton" mnemonicParsing="false" text="Show">
        <HBox.margin>
          <Insets left="120.0"/>
        </HBox.margin>
    </Button>
    <ImageView fx:id="leftActionImageView" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
        <HBox.margin>
          <Insets left="30.0"/>
        </HBox.margin>
    </ImageView>
    <ImageView fx:id="rightActionImageView" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
        <HBox.margin>
          <Insets left="20.0"/>
        </HBox.margin>
    </ImageView>
</HBox>
